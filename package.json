{
  "name": "frontend",
  "version": "1.0.0",
  "description": "front end task related",
  "main": "",
  "scripts": {
    "start": "npm run local",
    "prelocal": "pm2 kill",
    "local": "cross-env APP_SSR=true NODE_ENV=development PORT=7000 APP_PUBLIC_PATH=http://localhost:8080/build/client/ run-p local:*",
    "local:server-watch": "webpack --config ./webpack.server.babel.js --watch",
    "local:client-hotreload": "webpack-dev-server --config ./webpack.client.babel.js --hot",
    "local:multiple-node-server": "pm2 start pm2.json --only frontend-local",
    "postlocal": "npm stop",
    "development": "run-s development:*",
    "development:build": "run-s build:*",
    "development:start": "pm2 start pm2.json --only frontend-local",
    "build:client": "webpack --config ./webpack.client.babel.js --progress",
    "build:server": "webpack --config ./webpack.server.babel.js --progress",
    "pm2": "pm2"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "assets-webpack-plugin": "^3.9.6",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.4.0",
    "extract-css-chunks-webpack-plugin": "^3.1.1",
    "query-string": "^6.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.3.0",
